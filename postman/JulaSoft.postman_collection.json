						{
							"listen": "test",
							"script": {
								"exec": [
									"var body = pm.response.json();",
									"console.log(\"body: \", body);",
									"",
									"pm.test('token is valid', function () {",
									"",
									"    //Extract user token",
									"    var token = body.token",
									"    pm.expect(token).to.be.string;",
									"    // Set new User token",
									"    pm.environment.set(\"TOKEN\", \"Token \" + token);",
									"});"
								],
